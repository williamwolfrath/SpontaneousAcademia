  $terms = taxonomy_node_get_terms(node_load($nid));
  foreach($terms as $term){
    $sql = &quot;SELECT n.nid, n.title FROM {node} n INNER JOIN {term_node} tn ON n.nid = tn.nid WHERE n.status = 1 AND tn.tid = &quot;. $term-&gt;tid .&quot; AND n.nid != &quot;. $nid .&quot; ORDER BY n.created DESC LIMIT &quot; . $num_nodes;
    $result = db_query(db_rewrite_sql($sql));
    $output = node_title_list($result);
  }
  return $output;
}?&gt;</textarea>
 <div class="description">The content of the block as shown to the user.</div>
</div>
</span>

